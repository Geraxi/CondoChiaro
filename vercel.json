{
  "crons": [
    {
      "path": "/api/cron/daily-tasks",
      "schedule": "0 2 * * *"
    },
    {
      "path": "/api/cron/subscription-checks",
      "schedule": "0 6 * * *"
    },
    {
      "path": "/api/cron/cleanup-temp",
      "schedule": "0 3 * * *"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}







